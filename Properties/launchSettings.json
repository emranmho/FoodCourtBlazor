{
  "$schema": "https://json.schemastore.org/launchsettings.json",
    "profiles": {
      "http": {
        "commandName": "Project",
        "dotnetRunMessages": true,
        "launchBrowser": true,
        "hotReloadEnabled": true,
        "applicationUrl": "http://localhost:5291",
        "environmentVariables": {
          "ASPNETCORE_ENVIRONMENT": "Development"
        }
      },
      "https": {
        "commandName": "Project",
        "dotnetRunMessages": true,
        "launchBrowser": true,
        "applicationUrl": "https://localhost:7124;http://localhost:5291",
        "environmentVariables": {
          "ASPNETCORE_ENVIRONMENT": "Development"
        }
      },
      "Watch": {
        "commandName": "Executable",
        "workingDirectory": "$(ProjectDir)",
        "executablePath": "dotnet",
        "commandLineArgs": "watch run debug --launch-profile http"
        //        "commandLineArgs": "watch run --no-launch-profile --project $(ProjectFileName)"
      }
    }
  }
